{
  "name": "marten.klitzke.xyz",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "test": "yarn run test:unit",
    "test:unit": "vitest run --environment jsdom",
    "test:coverage": "vitest run --coverage --environment jsdom",
    "test:watch": "vitest watch --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "yarn run lint:eslint && yarn run lint:ts",
    "lint:eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:ts": "vue-tsc --noEmit -p tsconfig.app.json && vue-tsc --noEmit -p tsconfig.config.json && vue-tsc --noEmit -p tsconfig.vitest.json",
    "lint:fix": "yarn run lint:eslint --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/pro-duotone-svg-icons": "^6.2.0",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@headlessui/vue": "^1.7.2",
    "@heroicons/vue": "^2.0.11",
    "pinia": "^2.0.21",
    "pinia-plugin-persistedstate": "^2.2.0",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@pinia/testing": "^0.0.14",
    "@rushstack/eslint-patch": "^1.2.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.18",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.8.0",
    "eslint": "^8.23.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.5.1",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "sass": "^1.54.9",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.8.3",
    "vite": "^3.1.3",
    "vitest": "^0.28.4",
    "vue-tsc": "^0.40.7"
  }
}
